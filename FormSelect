namespace OPP_4
{
    public partial class FormSelect : Form
    {
        public FormSelect()
        {
            InitializeComponent();
        }

        private void btn_SaleTicket_Click(object sender, EventArgs e)
        {
            FormSaleTicket fmSale = new FormSaleTicket();
